{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "lint",
          "build",
          "build:client",
          "build:server",
          "generate"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "build:client": {
      "outputs": [
        "{projectRoot}/dist/client"
      ]
    },
    "build:server": {
      "outputs": [
        "{projectRoot}/dist/server"
      ]
    },
    "generate": {
      "outputs": [
        "{projectRoot}/dist/static"
      ]
    }
  }
}
