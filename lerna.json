{
  "version": "0.0.0",
  "npmClient": "yarn",
  "command": {
    "publish": {
      "ignoreChanges": ["*.md. packages/*/assets/**"],
      "message": "chore(release) publish"
    },
    "bootstrap": {
      "ignore": "component-*",
      "npmClientArgs": ["--no-package-lock"]
    }
  },
  "packages": ["packages/**"]
}
