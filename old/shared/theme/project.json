{
  "name": "shared-theme",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/shared/theme/src",
  "targets": {
    "document": {
      "executor": "@incremental.design/nx-plugin-shared:document",
      "dependsOn": ["build"],
      "outputs": ["dist/packages/shared/theme"],
      "options": {
        "repositoryUrl": "https://github.com/incremental-design/app-stencils"
      }
    },
    "build": {
      "executor": "@incremental.design/nx-plugin-shared:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/shared/theme",
        "bugs": "https://github.com/incremental-design/app-stencils/issues"
      },
      "dependsOn": ["test", "^build"]
    },
    "test": {
      "executor": "@incremental.design/nx-plugin-shared:test",
      "dependsOn": ["lint"]
    },
    "lint": {
      "executor": "@incremental.design/nx-plugin-shared:lint",
      "outputs": ["packages/shared/theme"],
      "dependsOn": ["format"]
    },
    "format": {
      "executor": "@incremental.design/nx-plugin-shared:format",
      "outputs": ["packages/shared/theme"]
    }
  },
  "tags": []
}
