{
  "name": "nx-plugin-vue3",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/nx-plugin/vue3/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/nx-plugin/vue3",
        "main": "packages/nx-plugin/vue3/src/index.ts",
        "tsConfig": "packages/nx-plugin/vue3/tsconfig.lib.json",
        "assets": [
          "packages/nx-plugin/vue3/*.md",
          {
            "input": "./packages/nx-plugin/vue3/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./packages/nx-plugin/vue3/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./packages/nx-plugin/vue3",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./packages/nx-plugin/vue3",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/nx-plugin/vue3/**/*.ts",
          "packages/nx-plugin/vue3/generators.json",
          "packages/nx-plugin/vue3/executors.json",
          "packages/nx-plugin/vue3/package.json"
        ]
      }
    }
  },
  "tags": []
}
