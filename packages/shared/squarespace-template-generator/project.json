{
  "name": "shared-squarespace-template-generator",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@incremental.design/nx-plugin-vue3:build",
      "options": {
        "viteConfig": "production"
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/packages/shared/squarespace-template-generator"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../coverage/packages/shared/squarespace-template-generator"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/shared/squarespace-template-generator/**/*.ts"
        ]
      }
    },
    "format": {
      "executor": "@incremental.design/nx-plugin-vue3:format",
      "outputs": ["{projectRoot}/**/*"]
    },
    "vite-build": {
      "executor": "@nrwl/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/packages/shared/squarespace-template-generator"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    }
  }
}
